config_version=5

[addons]

ss2d/version="3.2.0"

[application]

config/name="InfiniteRoom"
run/main_scene="uid://c1oo5q6thv8lw"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false

[autoload]

Log="*res://scr/singleton/log/log.gd"
Lib="*res://scr/lib/lib.gd"
RandLib="*res://scr/lib/rand_lib.gd"
ArrLib="*res://scr/lib/arr_lib.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
Console="*res://addons/godot-console/scripts/console.gd"
UIAnimation="*res://addons/godot_ui_animations/UIAnimationHandler.tscn"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/always_on_top=true
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/better-terrain/plugin.cfg", "res://addons/gdscript_formatter/plugin.cfg", "res://addons/godot-console/plugin.cfg", "res://addons/godot_ui_animations/plugin.cfg", "res://addons/label_font_auto_sizer/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/ui_design_tool/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "orange",
"res://asset/": "yellow",
"res://scr/": "teal",
"res://scr/role/blocks/types/": "blue",
"res://scr/role/items/type/": "blue",
"res://scr/role/room/theme/": "blue",
"res://xxx/": "gray"
}

